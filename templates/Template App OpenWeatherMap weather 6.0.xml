                <item>
                    <uuid>5610ed8c8f0949509b6b3fcf81e9a159</uuid>
                    <name>Is dark</name>
                    <type>DEPENDENT</type>
                    <key>is_dark</key>
                    <delay>0</delay>
                    <valuemap>
                        <name>Weather is dark status</name>
                    </valuemap>
                    <preprocessing>
                        <step>
                            <type>JAVASCRIPT</type>
                            <parameters>
                                <parameter>json_value = JSON.parse(value);
sunrise = json_value ['body']['sys']['sunrise'];
sunset = json_value['body']['sys']['sunset'];
dt = json_value['body']['dt']; //time of calculation

if(dt &gt; sunrise &amp;&amp; dt &lt; sunset) {
    return 0; // DAY
}
else if(dt &lt; sunrise || dt &gt; sunset){
    return 1; // NIGHT
}
else {
    return &quot;ZBX_NOTSUPPORTED&quot;;
}</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>get_weather.http</key>
                    </master_item>
                    <request_method>POST</request_method>
                    <tags>
                        <tag>
                            <tag>Application</tag>
                            <value>Weather</value>
                        </tag>
                    </tags>
                </item>
